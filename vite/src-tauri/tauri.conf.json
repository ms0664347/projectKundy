{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "kundy-app",
  "version": "0.1.0",
  "identifier": "com.kundy.app",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "kundy-app",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/kundy.jpg",
      "icons/kundy.ico",
      "icons/kundy.png"
    ],
    "resources": [
      ".env"
    ]
  }
}